	flow_down=rnd()<0.5
end
-- 1/3 flow left,right,both
flow_r=rnd()<0.5
flow_l=rnd()<0.5
-- sparks require stretch
sparks=rnd()<0.2 and stretch
extra_burn=rnd()<0.5
ext_brn_dir=-1+(rnd(2)\1)*2
alt_extra_burn=rnd()<0.5
mirror_left=rnd()<0.3
slow_flow=rnd()<0.5
if(slow_flow) slow_flow_d=1-(rnd(2)\1)*2
