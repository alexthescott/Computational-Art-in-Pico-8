	end
	
	if flow_up then
		memcpy(24576,24640,4096-off)
	end if flow_down then	
		memcpy(28672+off,28608+off,4096-off)
	end
	
	if sparks then	
		if flow_down then
			p=0x8000-4096+off
		else
			p=0x6000+4096-off
		end
			
		poke(p+64,peek(p))
		poke(p+ext_brn_dir,peek(p)-0.1)
