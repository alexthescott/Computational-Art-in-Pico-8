	print("bad burn",48,64,7)
elseif mirror_left and t()==2 then 
	poke(0x5f2c,5)
else
	if not extra_burn and 
	 big_line or full_line or 
	 alt_extra_burn then
			spawn_stripe=rnd()<(0.075+sin(a)+1)/2
	else
		spawn_stripe=rnd()<(0.25+(sin(a)+1)/2)
	end
	
	if(spawn_stripe) stripe()
	burn_v()
	
	fc+=1
	
