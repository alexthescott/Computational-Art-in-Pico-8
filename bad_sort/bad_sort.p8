 if(b[2]>=128or b[1]>=128)return false
 if d==0 then
 	return pget(a[1],a[2])>pget(b[1],b[2])
	else
		return pget(a[1],a[2])<pget(b[1],b[2])
	end
end

function sort(a,b)
	if cmp(a,b,1) then
		t=pget(a[1],a[2])
